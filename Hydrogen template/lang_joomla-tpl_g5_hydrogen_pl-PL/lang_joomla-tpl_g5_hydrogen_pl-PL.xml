<?xml version="1.0" encoding="utf-8"?>
<extension type="file" version="2.5" method="upgrade">

    <name>Spolszczenie szablonu Hydrogen</name>
    <author>Zbigniew Sokal</author>
    <creationDate>07 January 2022</creationDate>
