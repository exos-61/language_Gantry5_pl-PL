<?xml version="1.0" encoding="utf-8"?>
<extension type="file" version="2.5" method="upgrade">

    <name>Spolszczenie pakietu Gantry 5</name>
    <author>Zbigniew Sokal</author>
    <creationDate>12 January 2023</creationDate>
    <copyright>(C) 2018-2022 Poradniki Dziadka</copyright>
    <license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
    <authorEmail>admin@dziadek.guide</authorEmail>
    <authorUrl>https://dziadek.guide</authorUrl>
    <version>5.5.15</version>
    <description>Spolszczenie pakietu Joomla Gantry 5</description>

    <!-- Fileset definition -->
    <fileset>

        <!-- back-end -->
        <files folder="admin/pl-PL" target="administrator/components/com_gantry5/language/pl-PL">
            <filename>pl-PL.com_gantry5.ini</filename>
            <filename>pl-PL.com_gantry5.sys.ini</filename>
        </files>

        <files folder="admin/pl-PL" target="plugins/gantry5/preset/language/pl-PL">
            <filename>pl-PL.plg_gantry5_preset.sys.ini</filename>
        </files>

        <files folder="admin/pl-PL" target="plugins/quickicon/gantry5/language/pl-PL">
            <filename>pl-PL.plg_quickicon_gantry5.sys.ini</filename>
        </files>

        <files folder="admin/pl-PL" target="plugins/system/gantry5/language/pl-PL">
            <filename>pl-PL.plg_system_gantry5.sys.ini</filename>
        </files>

        <!-- front-end -->
        <files folder="site/pl-PL" target="modules/mod_gantry5_particle/language/pl-PL">
            <filename>pl-PL.mod_gantry5_particle.ini</filename>
            <filename>pl-PL.mod_gantry5_particle.sys.ini</filename>
        </files>

        <files folder="site/pl-PL" target="libraries/gantry5/language/pl-PL">
            <filename>pl-PL.lib_gantry5.sys.ini</filename>
        </files>

        <files folder="site/pl-PL" target="language/pl-PL">
            <filename>pl-PL.files_gantry5_nucleus.ini</filename>
            <filename>pl-PL.files_gantry5_nucleus.sys.ini</filename>
            <filename>pl-PL.pkg_gantry5.sys.ini</filename>
        </files>
        <!-- Readme -->
        <files folder="CZYTAJ" target="SPOLSZCZENIE_GANTRY5_SZABLON/CZYTAJ">
            <filename>CZYTAJ.txt</filename>
        </files>
        <!-- Custom -->
        <!-- administrator -> custom/admin -->
        <files folder="custom/admin/blueprints/gantry/theme" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/admin/blueprints/gantry/theme">
            <filename>configuration.yaml</filename>
            <filename>details.yaml</filename>
        </files>

        <files folder="custom/admin/blueprints/menu" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/admin/blueprints/menu">
            <filename>menu.yaml</filename>
            <filename>menuitem.yaml</filename>
        </files>

        <!-- media -> custom/engine -->
        <files folder="custom/engine/admin/blueprints/layout/inheritance/messages" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/admin/blueprints/layout/inheritance/messages">
            <filename>default.yaml</filename>
            <filename>empty.yaml</filename>
            <filename>inherited.yaml</filename>
        </files>

        <files folder="custom/engine/admin/blueprints/layout/inheritance" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/admin/blueprints/layout/inheritance">
            <filename>atom.yaml</filename>
            <filename>offcanvas.yaml</filename>
            <filename>particle.yaml</filename>
            <filename>position.yaml</filename>
            <filename>section.yaml</filename>
            <filename>system.yaml</filename>
        </files>

        <files folder="custom/engine/admin/blueprints/layout" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/admin/blueprints/layout">
            <filename>block.yaml</filename>
            <filename>container.yaml</filename>
            <filename>offcanvas.yaml</filename>
            <filename>section.yaml</filename>
        </files>

        <files folder="custom/engine/admin/blueprints/menu" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/admin/blueprints/menu">
            <filename>block.yaml</filename>
        </files>

        <files folder="custom/engine/admin/blueprints/position" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/admin/blueprints/position">
            <filename>chrome.yaml</filename>
        </files>

        <files folder="custom/engine/blueprints/page" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/blueprints/page">
            <filename>assets.yaml</filename>
            <filename>body.yaml</filename>
            <filename>fontawesome.yaml</filename>
            <filename>head.yaml</filename>
        </files>

        <files folder="custom/engine/particles" target="SPOLSZCZENIE_GANTRY5_SZABLON/custom/engine/particles">
            <filename>analytics.yaml</filename>
            <filename>assets.yaml</filename>
            <filename>branding.yaml</filename>
            <filename>content.yaml</filename>
            <filename>contentarray.yaml</filename>
            <filename>copyright.yaml</filename>
            <filename>custom.yaml</filename>
            <filename>date.yaml</filename>
            <filename>frameworks.yaml</filename>
            <filename>lightcase.yaml</filename>
            <filename>logo.yaml</filename>
            <filename>menu.yaml</filename>
            <filename>messages.yaml</filename>
            <filename>mobile-menu.yaml</filename>
            <filename>module.yaml</filename>
            <filename>position.yaml</filename>
            <filename>social.yaml</filename>
            <filename>spacer.yaml</filename>
            <filename>totop.yaml</filename>
        </files>

    </fileset>
    <updateservers>
        <server type="collection" priority="1" name="lang_joomla_gantry5_pl-PL">https://dziadek.guide/langpack/translationlist.xml</server>
    </updateservers>
</extension>
